{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "dictionaries": ["typescript", "node", "html", "css", "fonts"],
  "words": [
    "bitget",
    "bitkeep",
    "Bitski",
    "dequal",
    "Desig",
    "enkrypt",
    "evmproviders",
    "Fordefi",
    "Haqq",
    "okex",
    "okxwallet",
    "Onekey",
    "qrcode",
    "Rabby",
    "rabbykit",
    "Taho",
    "Tokenary",
    "tpoutside",
    "trustwallet",
    "viem",
    "wagmi",
    "xdefi",
    "Yahei",
    "zerion",
    "zustand"
  ],
  "ignoreWords": ["mipd", "outroend"],
  "ignoreRegExpList": ["/[A-Za-z0-9]{46}/"],
  "ignorePaths": [
    "*.svg",
    "node_modules",
    ".github",
    ".gitignore",
    ".prettierignore",
    "*.snap",
    "**/*.json",
    "**/*.tsbuildinfo",
    "**/dist/**",
    "**/node_modules/**",
    "*generated*",
    "patches/**",
    "pnpm-lock.yaml"
  ]
}
